num = int(input("Please enter any integer to find divisors = "))

print("The Divisors of the Number = ")
count=0
for i in range(1, num + 1):
    if num % i == 0:
        if i==num: 
             print(i,end='')
             count+=1
        else:
            print(i,end=',')
            count+=1
print()
print(count)