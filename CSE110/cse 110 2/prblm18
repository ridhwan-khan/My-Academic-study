row = int(input("Enter number of rows: "))
col = int(input("Enter number of columns: "))

print("Rectangular pattern is: ")
for i in range(1,row+1):
    for j in range(1,col+1): #vetorer loop age run krtese pasapasi outpuut ditese 
        print(j, end="") #otput pasapasi print hocche 

    print() ## ekta none print krsi jate loop ta amr asapasi output na dekhay so porer line e chole jabe 
    