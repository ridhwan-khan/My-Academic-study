##task 13
n= int (input ('enter a number ')) #6
count=0 #1,2,3,4
i=1 #1,2,3,4,5,6,7
while i!=n+1: #not equal 7
    
    if n%i==0:#remainder #0 ,0,0,0
       if i==n: #false,F,TRUE
        print(i,end='') 
        i+=1 #7
        count+=1 #4
       else:
        print(i,end=',') #1,2,3,6
        count+=1 #1,2,3
        i+=1 #2,3,4
    else: 

        i+=1 #5,6
print()
print('Total',count,'divisors')


